        <%= link_to current_user.username %>
        <%= link_to t("layouts.edit"), "#" %>
      </div>
      <ul>
        <li>
          <img src="/assets/newfeed.png" alt="<%= t "layouts.post" %>">
          <%= link_to t("layouts.post"), "#" %>
        </li>
        <li>
          <img src="/assets/timeline.png" alt="<%= t "layouts.timeline" %>">
          <%= link_to t("layouts.timeline"), "#" %>
        </li>
        <li>
          <img src="/assets/friend.png" alt="<%= t "layouts.user" %>">
          <%= link_to t("layouts.user"), "#" %>
        </li>
        <li>
          <img alt="" class="v-sm-img" src="#"
            title="<%= t "layouts.profile" %>"><%= t"layouts.profile" %>
