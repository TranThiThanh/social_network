          <img src="/assets/help.png" alt="<%= t "layouts.help" %>">
          <%= link_to t("layouts.help"), "#" %>
        </li>
        <li>
          <img src="/assets/privacy.png" alt="<%= t "layouts.privacy" %>">
          <%= link_to t("layouts.privacy"), "#" %>
        </li>
        <li>
          <img src="/assets/download.png" alt="<%= t "layouts.service" %>">
          <%= link_to t("layouts.service"), "#" %>
